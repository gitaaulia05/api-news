        "/admin/login": {
            "post": {
              "description": "Validasi Administrator akan login ",
              "parameters": [
                {
                  "name": "Authorization",
                  "in": "header",
                  "required": true,
                  "schema": {
                    "type": "string"
                  },
                  "description": "Bearer token untuk autentikasi"
                }
              ],
              "responses": {
                "200": {
                  "description": "Berhasil Login",
                  "content": {
                    "application/json": {
                      "schema": {
                        "type": "object",
                        "properties": {
                          "data": {
                            "type": "object",
                            "properties": {
                              "email": {
                                "type": "string",
                                "format": "email"
                              },
                              "password": {
                                "type": "string",
                                  "format": "password"
                              }
                            }
                          },
                          "errors": {
                            "type": "object"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
         },
        "/admin": {
  "get": {
  
    "description": "Mengambil informasi admin yang sedang login",
    "parameters": [
      {
        "name": "Authorization",
        "in": "header",
        "required": true,
        "schema": {
          "type": "string"
        },
        "description": "Bearer token untuk autentikasi"
      }
    ],
    "responses": {
      "200": {
        "description": "Data admin berhasil diambil",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "data": {
                  "type": "object",
                  "properties": {
                    "nama": {
                      "type": "string"
                    },
                    "email": {
                      "type": "string",
                      "format": "email"
                    },
                    "slug": {
                      "type": "string"
                    },
                    "gambar": {
                      "type": "string",
                      "format": "uri",
                      "description": "URL gambar admin"
                    },
                    "token": {
                      "type": "string"
                    },
                    "role": {
                      "type": "string"
                    },
                    "active": {
                      "type": "boolean"
                    }
                  }
                },
                "errors": {
                  "type": "object"
                }
              }
            }
          }
        }
      }
    }
  }
