         "/berita/{slugBerita}": {
            "get": {
              "description": "Menampilkan Data Berita Secara Spesifik",
              "parameters": [
                {
                  "name": "Authorization",
                  "in": "header",
                  "required": true,
                  "schema": {
                    "type": "string"
                  },
                  "description": "Bearer token untuk autentikasi"
                }
              ],
              "responses": {
                "200": {
                  "description": "Data Detail Berita  Berhasil Diambil",
                  "content": {
                    "application/json": {
                      "schema": {
                        "type": "object",
                        "properties": {
                          "data": {
                            "type":"object",
                            "properties":{
                                "id_administrator":{
                                    "type":"string"
                                },
                                "nama_jurnalis":{
                                    "type":"string"
                                },
                                "slug":{
                                    "type":"string"
                                },
                                "judul_berita":{
                                    "type":"string"
                                },
                                "deks_berita":{
                                    "type":"string"
                                },
                                "is_tayang":{
                                    "type":"boolean"
                                },
                                "created_at":{
                                    "type":"string"
                                },
                                "simpanBerita":{
                                    "type":"string"
                                },
                                "updated_at":{
                                    "type":"string"
                                },
                                "kategori_berita":{
                                    "type":"string"
                                },
                                "gambar":{
                                    "type": "array",
                                    "items": {
                                      "type": "object",
                                      "properties": {
                                        "gambar_berita": {
                                          "type": "string",
                                          "description": "URL atau path gambar"
                                        },
                                        "keterangan_gambar": {
                                          "type": "string",
                                          "description": "Deskripsi atau keterangan dari gambar"
                                        }
                                      }
                                    }
                                }
                            }
                          },
